{
	"compilerOptions": {
		"module": "NodeNext",
		"checkJs": true,
		"allowJs": true,
		"target": "ESNext",
		"moduleResolution": "NodeNext",
		"strict": true,
		"noImplicitThis": true,
		"composite": true,
		"emitDeclarationOnly": true,
		"alwaysStrict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"incremental": true,
		"declaration": true,
		"declarationMap": true
	}
}

{
	"name": "@trvt/core",
	"version": "0.9.12",
	"description": "Core loader for Trivet",
	"homepage": "https://github.com/dutchcelt/trivet#readme",
	"license": "AGPL-3.0",
	"author": "C. Egor Kloos",
	"type": "module",

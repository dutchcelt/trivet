{
	"name": "@trvt/datatable",
	"version": "0.3.7",
	"description": "Static datatable",
	"keywords": [
		"web",
		"components"
	],
	"license": "AGPL-3.0",
	"author": "C. Egor Kloos",
	"type": "module",
	"exports": {
		".": "./index.js",
		"./*": {
			"default": "./dist/*",
			"types": "./dist/types/*"
		}
	},
	"main": "index.js",
	"files": [
		"src",
		"dist"
	],
	"scripts": {
		"build": "npm run build:rollup",
		"build:rollup": "npx -y rollup -c \"../../../configs/rollup.config.js\"",
		"develop": "npm run build && npx -y wds --root-dir \"../../\" --watch --node-resolve --open \"/webcomponents/datatable/index.html\"",
		"types": "npx -y tsc -p tsconfig.json"
	},
	"dependencies": {
		"@trvt/core": "^0.9.7"
	}
}

		"./*": {
			"default": "./dist/*",
			"types": "./dist/types/*"
		}
	},
	"type": "module",
	"keywords": [
		"web",
		"components"
	],
	"author": "C. Egor Kloos",
	"license": "AGPL-3.0",
	"dependencies": {

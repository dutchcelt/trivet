{
	"name": "@trvt/contenttoggle",
	"version": "0.4.6",
	"description": "Toggle content as an accordion or as tabs",
	"keywords": [
		"web",
		"components"
	],
	"license": "AGPL-3.0",
	"author": "C. Egor Kloos",
	"type": "module",
	"exports": {
		".": "./index.js",
		"./*": {
			"default": "./dist/*",
			"types": "./dist/types/*"
		}
	},
	"main": "index.js",
	"files": [
		"src",
		"dist"
	],
	"scripts": {
		"build": "npm run build:rollup",
		"build:rollup": "npx rollup -c ../../../configs/rollup.config.js",
		"develop": "npm run build:rollup --watch",
		"types": "npx -y tsc -p tsconfig.json"
	},
	"dependencies": {
		"@trvt/core": "^0.9.7"
	}
}

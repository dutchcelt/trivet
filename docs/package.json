		"@trvt/dragscroller": "^0.3.5",
		"@trvt/header": "^0.8.4",
		"@trvt/layout": "^0.8.5",
		"@trvt/lists": "^0.4.7",
		"@trvt/styles": "^0.0.0",
		"@trvt/utils": "^0.0.1",
		"lightningcss": "^1.22.0"

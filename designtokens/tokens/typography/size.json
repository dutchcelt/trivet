{
	"typography": {
		"size": {
			"root": {
				"$value": "112.5%",
				"comment": "set root to 18px"
			},
			"xxxl": {
				"$value": "xxx-large"
			},
			"xxl": {
				"$value": "xx-large"
			},
			"xl": {
				"$value": "x-large"
			},
			"l": {
				"$value": "large",
				"$extensions": {
					"trvt.css.fluid": {
						"minWidth": "480",
						"maxWidth": "1200",
						"minSize": "16",
						"maxSize": "18"
					}
				}
			},
			"m": {
				"$value": "medium"
			},
			"s": {
				"$value": "small"
			},
			"xs": {
				"$value": "x-small"
			},
			"xxs": {
				"$value": "xx-small"
			}
		}
	}
}

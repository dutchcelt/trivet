{
	"parser": "@babel/eslint-parser",
	"env": {
		"browser": true,
		"es2022": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"requireConfigFile": false,
		"babelOptions": {
			"plugins": ["@babel/plugin-syntax-import-assertions"]
		}
	},
	"ignorePatterns": ["**/vendor/*.js", "**/build/*.js", "**/tests/*.js", "**/docs/scripts/*.js", "**/assets/**/*"],
	"rules": {}
}
